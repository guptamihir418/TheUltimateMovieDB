{
  "name": "the-ultimate-moviedb--frontend",
  "version": "0.0.0",
  "private": "true",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "build:react": "react-scripts build",
    "build:tailwind": "npm run tailwind -- --env=production",
    "build": "run-p -l build:*",
    "tailwind": "postcss src/styles.css -o src/output.css",
    "dev:react": "react-scripts start",
    "dev:tailwind": "npm run tailwind -- -w",
    "dev": "run-p dev:*",
    "start": "npm run dev"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@tailwindcss/custom-forms": "^0.2.1",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "clsx": "^1.1.1",
    "formik": "^2.2.5",
    "http-proxy-middleware": "^1.0.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.4",
    "semantic-ui-react": "^2.0.1",
    "swr": "^0.3.9",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "autoprefixer": "^9.8.6",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.0",
    "tailwindcss": "^1.9.6",
    "typescript": "^3.9.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "create-react-app",
    "TypeScript",
    "TailwindCSS",
    "@reach/router"
  ]
}
