{
  "name": "the-ultimate-moviedb",
  "version": "1.0.0",
  "description": "COMP2406 Fall 2020 Final Project",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "https://github.com/saihaj/TheUltimateMovieDB.git"
  },
  "bugs": {
    "url": "https://github.com/saihaj/TheUltimateMovieDB/issues"
  },
  "scripts": {
    "postinstall": "run-p -l install:*",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start": "run-p -l start:*",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "run-p -l dev:*",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build": "run-p -l build:*",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "lint": "run-p -l lint:*",
    "clean:backend": "cd backend && npm run clean",
    "clean": "run-p -l clean:*"
  },
  "contributors": [
    "Saihajpreet Singh <saihajpreet.singh@gmail.com>",
    "Mihir Gupta <guptamihir418@gmail.com>"
  ],
